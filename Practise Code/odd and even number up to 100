//odd number up to 100

class Main {
    public static void main(String[] args) {
        System.out.println("odd number up to 100:");
        for(int i=1;i<=100;i+=2){
            System.out.print(i+ " ");
        }
    }
}

//even number up to 100


class Main {
    public static void main(String[] args) {
        System.out.println("even number upto 100:");
        for(int i=2;i<=100;i+=2){
            System.out.print(i+ " ");
        }
    }
}
